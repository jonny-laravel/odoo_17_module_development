<odoo>
    <!-- view tree -->
    <record id="trans_det_anggota_kel_view_tree" model="ir.ui.view">
        <field name="name">trans.det.anggota.kel.view.tree</field>
        <field name="model">trans.det.anggota.kelr</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="det_marga_id"/>
                <field name="tempat_lahir" />
                <field name="tanggal_lahir" />
                <field name="jenis_kelamin" />
                <field name="status_keluarga" />
            </tree>
        </field>
    </record>
    <!-- Form -->
    <record id="trans_det_anggota_kel_view_form" model="ir.ui.view">
        <field name="name">trans.det.anggota.kel.view.form</field>
        <field name="model">trans.det.anggota.kel</field>
        <field name="arch" type="xml">
            <form string= "Anggota Keluarga">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="det_marga_id"/>
                        <field name="tempat_lahir" />
                        <field name="tanggal_lahir" />
                        <field name="jenis_kelamin" />
                        <field name="status_keluarga" />
                    </group>
                </sheet>
            </form >
        </field>
    </record>

</odoo>